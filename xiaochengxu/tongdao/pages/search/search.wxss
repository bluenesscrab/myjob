/**search.wxss**/
page {
  display: block;
  min-height: 100%;
  font-size:14px;
  background-color:white;
}
.test{
  color:red;
}
/* public */
.float-right{
  float:right;
}
.mid{
  display: inline-block;
  vertical-align: middle;
}
.labels{
  border-radius:4px;
  padding:6px 12px;
  margin:5px;
  color:#6c6c6c;
  font-size:12px;
  background-color:#f2f2f2;
  float: left;
}
.labels text{
  white-space:nowrap;
}
.flex-wrap{
  display:flex;
  flex-direction:row;
}
.flex-item{
  display: flex;
  flex: 1;
  align-items: center;
}
.sections{
  padding:10px 15px 5px;
}
.sections .titles{
  color:#999;
  display:block;
  padding-bottom:5px;
}
.sections .content:after{
  content:'';
  display:block;
  clear:both;
}
.lists .lis{
  border-bottom:1px solid #e8e8e8;
}
.lists .lis:first-child{
  border-top:1px solid #e8e8e8;
}
.sections.lists .lis .lis-icon{
  height:18px;
  padding-right:10px;
}
.sections.lists .lis text{
}
.sections.lists .lis .cont{
  padding:12px 0;
}
.sections.lists .lis .iconfont{
  color:#666;
}
.sections.lists .lis .remove{
  justify-content: center;
}
.flex-2{
  flex:2;
}
.flex-3{
  flex:3;
}
.flex-4{
  flex:4;
}
.flex-5{
  flex:5;
}
.flex-6{
  flex:6;
}
.flex-7{
  flex:7;
}
/* end public */

/* search-bar */
.search-bar{
  padding:6px 10px;
  font-size: 14px;
  border-bottom:1px solid #b9b9b9;
  color:#333;
}
.search-bar .search-parent{
  padding:0 30px 0 10px;
  height:32px;
  border-radius:16px;
  position:relative;
  background-color:#e9e9e9;
}
.search-bar .search-parent form{
  width:100%;
}
.search-bar .search-parent .input-text{
  width:100%;
  height:32px;
}
.search-bar .search-parent .search-btn{
  position:absolute;
  top:0;
  right:0;
  background:none;
  width:36px;
  height:100%;
  line-height:32px;
  padding:0;
}
.search-bar .search-parent .search-btn .icon-search{
  font-size:20px;
}
.search-bar .search-parent .search-btn::after{
  border-color:transparent;
}
.search-bar .right-btn{
  justify-content: center;
}
/* end search-bar */